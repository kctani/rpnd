{
  "rpnd": {
    "description": "Grant uci r/w access, file r/w access to /tmp/rpnd/*",
    "read": {
      "uci": ["rpnd"]
    },
    "write": {
      "uci": ["rpnd"],
      "cgi-io": ["upload"],
      "file": {
        "/tmp/rpnd/status": ["read"],
        "/tmp/rpnd/config": ["read"],
        "/tmp/rpnd/mqtt-in": ["write"]
      }
    }
  }
}
